---
const astro: Record<string, any> = JSON.parse(JSON.stringify(Astro));
try {
  astro.cookies = Astro.cookies;
} catch {}
try {
  astro.props = Astro.props;
} catch {}

try {
  astro.misc = {};
  astro.misc.clientId = __GITHUB_CLIENT_ID;
  astro.misc.Fragment = Fragment;
} catch {}
---

<details>
  <summary><code>meta</code></summary>
