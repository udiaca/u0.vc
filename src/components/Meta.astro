---
const astro: Record<string, any> = JSON.parse(JSON.stringify(Astro));
try {
  // https://docs.astro.build/en/reference/api-reference/#astrocookies
  astro.cookies = Array.from(Astro.cookies.headers());
} catch {}
try {
  astro.props = Astro.props;
} catch {}
