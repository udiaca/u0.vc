---
const astro: Record<string, any> = JSON.parse(JSON.stringify(Astro));
try {
  // https://docs.astro.build/en/reference/api-reference/#astrocookies
  astro.cookies = Array.from(Astro.cookies.headers());
} catch {}

try {
  astro.request.headers = {}
  Astro.request.headers.forEach((value, key) => {
    astro.request.headers[key] = value;
  })
} catch {}

try {
  astro.props = Astro.props;
} catch {}

try {
  astro.misc = {};
  astro.misc.clientId = __GITHUB_CLIENT_ID;
  astro.misc.Fragment = Fragment;
} catch {}
---

<details>
  <summary><code>meta</code></summary>
  <code>
    <pre>{JSON.stringify(astro, undefined, 2)}</pre>
  </code>
</details>
<style>
  pre {
    max-width: 100vw;
    overflow-x: auto;
    border: 1px dashed rebeccapurple;
  }
  details {
    padding-bottom: 1rem;
  }
</style>
